<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.integrator.mapper.InAppAccessForwardMapper">
    
    <resultMap type="InAppAccessForward" id="InAppAccessForwardResult">
        <result property="appFwdId"    column="app_fwd_id"    />
        <result property="appId"    column="app_id"    />
        <result property="fwdId"    column="fwd_id"    />
        <result property="status"    column="status"    />
        <result property="remark"    column="remark"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
    </resultMap>

    <sql id="selectInAppAccessForwardVo">
        select app_fwd_id, app_id, fwd_id, status, remark, create_by, create_time, update_by, update_time from in_app_access_forward
    </sql>

    <select id="selectInAppAccessForwardList" parameterType="InAppAccessForward" resultMap="InAppAccessForwardResult">
        <include refid="selectInAppAccessForwardVo"/>
        <where>  
            <if test="appId != null  and appId != ''"> and app_id = #{appId}</if>
            <if test="fwdId != null  and fwdId != ''"> and fwd_id = #{fwdId}</if>
            <if test="status != null  and status != ''"> and status = #{status}</if>
        </where>
    </select>
    
    <select id="selectInAppAccessForwardById" parameterType="String" resultMap="InAppAccessForwardResult">
        <include refid="selectInAppAccessForwardVo"/>
        where app_fwd_id = #{appFwdId}
    </select>
        
    <insert id="insertInAppAccessForward" parameterType="InAppAccessForward">
        insert into in_app_access_forward
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="appFwdId != null">app_fwd_id,</if>
            <if test="appId != null">app_id,</if>
            <if test="fwdId != null">fwd_id,</if>
            <if test="status != null">status,</if>
            <if test="remark != null">remark,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="appFwdId != null">#{appFwdId},</if>
            <if test="appId != null">#{appId},</if>
            <if test="fwdId != null">#{fwdId},</if>
            <if test="status != null">#{status},</if>
            <if test="remark != null">#{remark},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
         </trim>
    </insert>

    <update id="updateInAppAccessForward" parameterType="InAppAccessForward">
        update in_app_access_forward
        <trim prefix="SET" suffixOverrides=",">
            <if test="appId != null">app_id = #{appId},</if>
            <if test="fwdId != null">fwd_id = #{fwdId},</if>
            <if test="status != null">status = #{status},</if>
            <if test="remark != null">remark = #{remark},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where app_fwd_id = #{appFwdId}
    </update>

    <delete id="deleteInAppAccessForwardById" parameterType="String">
        delete from in_app_access_forward where app_fwd_id = #{appFwdId}
    </delete>

    <delete id="deleteInAppAccessForwardByIds" parameterType="String">
        delete from in_app_access_forward where app_fwd_id in 
        <foreach item="appFwdId" collection="array" open="(" separator="," close=")">
            #{appFwdId}
        </foreach>
    </delete>
    
</mapper>