<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.demo.mapper.DemoAccountWaterTccMapper">

    <select id="selectDemoAccountWaterTccBySerialNumber" parameterType="String" resultMap="DemoAccountWaterTccResult">
        <include refid="selectDemoAccountWaterTccVo"/>
        where serial_number = #{serialNumber}
    </select>
    
</mapper>